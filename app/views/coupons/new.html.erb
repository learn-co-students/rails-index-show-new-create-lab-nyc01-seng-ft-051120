<h1>New Coupon Form</h1>
<%= form_tag coupons_path do %>
<label>Store Name</label><br>
<%= text_field_tag :'coupon[store]' %><br><br>
<label>Coupon Code</label><br>
<%= text_field_tag :'coupon[coupon_code]' %><br>
<%= hidden_field_tag :authenticity_token, form_authenticity_token %><br><br>
<%= submit_tag "Submit Coupon" %>
<% end %>